{
  "name": "se.dbwebb.bikeapp",
  "displayName": "Bike App",
  "version": "1.0.0",
  "description": "A sample Apache Cordova application that responds to the deviceready event.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "style": "sass www/css/base.scss www/dist/style.css --no-source-map",
    "style-min": "sass www/css/base.scss www/dist/style.min.css --no-source-map --style compressed",
    "start": "webpack --watch --config webpack.dev.config.js",
    "build": "webpack --config webpack.prod.config.js"
  },
  "keywords": [
    "ecosystem:cordova"
  ],
  "author": "Benaris Hajduk",
  "license": "Apache-2.0",
  "devDependencies": {
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "cordova-plugin-geolocation": "^4.1.0",
    "cordova-plugin-whitelist": "^1.3.4",
    "css-loader": "^5.2.4",
    "file-loader": "^6.2.0",
    "sass": "^1.32.12",
    "style-loader": "^2.0.0",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.6.0"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-geolocation": {
        "GPS_REQUIRED": "true"
      }
    },
    "platforms": [
      "android",
      "browser"
    ]
  },
  "dependencies": {
    "cordova-android": "9.1.0",
    "cordova-browser": "6.0.0",
    "cordova-plugin-splashscreen": "6.0.0",
    "leaflet": "^1.7.1",
    "leaflet-geosearch": "^3.3.2",
    "mithril": "^2.0.4"
  }
}